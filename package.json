{
  "name": "ts-decode",
  "version": "0.0.5",
  "description": "type safe decoding in typescript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "author": "Alessandro Scandone",
  "repository": {
    "type": "git",
    "url": "https://github.com/ascandone/ts-decode"
  },
  "keywords": [
    "typescript",
    "decode",
    "parsing"
  ],
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "linter:check": "prettier src/* test/* --check",
    "typescript:check": "tsc --noEmit",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "add": "^2.0.6",
    "jest": "^27.4.3",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2"
  }
}
